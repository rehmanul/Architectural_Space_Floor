# Production Environment Variables for Render.com
NODE_ENV=production
PORT=10000

# Database (will be injected by Render)
# DATABASE_URL=postgresql://...

# File Upload Limits
MAX_FILE_SIZE=52428800
UPLOAD_TIMEOUT=30000

# ML Optimization Settings
ML_TIMEOUT=120000
OPTIMIZATION_MAX_ITERATIONS=100

# Security
CORS_ORIGIN=*
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX=100

# Performance
CACHE_TTL=300
COMPRESSION_LEVEL=6